    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "ZModDynamics",
            "email": "info@zmoddynamics.com",
            "homepage": "//zmoddynamics.com"
        }
    ],
    "require": {
        "kartik-v/yii2-widget-activeform": ">=1.5.7",
